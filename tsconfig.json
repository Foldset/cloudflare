{
  "extends": "../config/tsconfig.base.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "declarationMap": true
  },
  "include": [
    "src/**/*.ts",
    "types/worker-configuration.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}