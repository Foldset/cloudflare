{
  "name": "@foldset/cloudflare",
  "version": "0.0.1",
  "description": "Cloudflare Workers implementation for Foldset x402 payment protection",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./hono": {
      "types": "./dist/hono.d.ts",
      "default": "./dist/hono.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "bundle": "node esbuild.bundle.mjs",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@foldset/core": "github:foldset/core",
    "@x402/core": "^2.2.0",
    "@x402/hono": "^2.2.0",
    "@sentry/cloudflare": ">=10"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260117.0",
    "@eslint/js": "^9.20.0",
    "@sentry/cloudflare": "^10.34.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.1.0",
    "hono": "^4.11.4",
    "esbuild": "^0.25.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.25.0"
  }
}
